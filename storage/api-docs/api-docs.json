{
    "openapi": "3.0.0",
    "info": {
        "title": "Space Cargo Api",
        "version": "1.0"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Authentification"
                ],
                "summary": "Get a JWT access token via given credentials.",
                "operationId": "dd51a484b7fa0f97c7680fdbde14cf5f",
                "requestBody": {
                    "description": "Create customers object",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "username": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "testUserName"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "123456"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "success",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "bad request",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "validation",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/api/parcels/create": {
            "post": {
                "tags": [
                    "Parcels"
                ],
                "summary": "Create parcel",
                "operationId": "832cfda815dfe14aaa0cdc7119183a7b",
                "requestBody": {
                    "description": "Data for creating a new parcel",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "code": {
                                        "type": "string",
                                        "example": "AC44BB13#TT"
                                    },
                                    "price": {
                                        "type": "numeric",
                                        "example": "12.33"
                                    },
                                    "quantity": {
                                        "type": "numeric",
                                        "example": "12"
                                    },
                                    "store_address": {
                                        "type": "string",
                                        "example": "test address"
                                    },
                                    "comment": {
                                        "type": "string",
                                        "example": "test comment"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "apiAuth": []
                    }
                ]
            }
        },
        "/api/users": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get a list of users",
                "operationId": "20af7adfef3e7063b8a9dd7d20e5953c",
                "responses": {
                    "200": {
                        "description": "List of users"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "apiAuth": {
                "type": "http",
                "description": "Login with email and password to get the authentication token",
                "name": "Token based Based",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}